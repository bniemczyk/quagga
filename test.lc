fact = 
    letrec f = lambda n . if n == 1 then 1 else n * f (n - 1) in
    f ;

trip = lambda a b c . { a, b, c } ;

hundred = 
    let { x, y, z } = trip (2 * 20) 40 20 in 
    (x * y) + z ;

sqr = lambda n . n * n ;

twenty_five = let sqr = lambda n . n * n in sqr 5 ;

main = 
    putStrLn (show (fact (sqr 2) + hundred)) ;
